# Test Random Dice!
import random
n1=0
n2=0
n3=0
n4=0
n5=0
n6=0
print(random.randint(1,6))
for i in range(12000):
    a=random.randint(1,6)
    print(i)
    if a == 1:
        n1 += 1
    if a == 2:
        n2 += 1
    if a == 3:
        n3 += 1
    if a == 4:
        n4 += 1
    if a == 5:
        n5 += 1
    if a == 6:
        n6 += 1
print('result',n1,n2,n3,n4,n5,n6)
print('result',n1/12000*100,n2/12000*100,n3/12000*100,n4/12000*100,n5/12000*100,n6/12000*100)

